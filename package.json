  "engines": {
    "node": "22.20.0"
  },
  "build": {
    "appId": "org.difft.chative-desktop",
    "protocols": {
      "name": "weekly_protocol_name",
      "schemes": [
        "chative",
        "temptalk"
      ]
    },
    "afterPack": "ts/build/after-pack.js",
    "afterSign": "ts/build/after-sign.js",
    "mac": {
      "artifactName": "${name}-mac-${arch}-latest.${ext}",
      "category": "public.app-category.social-networking",
      "icon": "build/icons/mac/icon.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "releaseInfo": {
        "vendor": {
          "minOSVersion": "20.0.0"
        }
      },
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "bundleVersion": "1"
    },
    "pkg": {
      "isRelocatable": false,
      "isVersionChecked": false
    },
    "dmg": {
      "sign": false
    },
    "win": {
      "artifactName": "${name}-win-latest.${ext}",
      "icon": "build/icons/win/icon.ico",
      "releaseInfo": {
        "vendor": {
          "minOSVersion": "10.0.10240"
        }
      },
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "category": "Network;InstantMessaging;Chat",
      "artifactName": "${name}-${os}-${arch}-latest.${ext}",
      "executableName": "Yelling",
      "target": [
        {
          "target": "rpm",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icons/png"
    },
    "deb": {
      "packageName": "yelling",
      "depends": [
        "libnotify4",
        "libxtst6",
        "libnss3",
        "libasound2",
        "libxss1"
      ]
    },
    "rpm": {
      "packageName": "yelling",
      "fpm": [
        "--rpm-rpmbuild-define=_build_id_links none"
      ]
    },
    "asarUnpack": [
      "**/*.node"
    ],
    "files": [
      "package.json",
      "config/default.json",
      "config/${env.SIGNAL_ENV}.json",
      "config/local-${env.SIGNAL_ENV}.json",
      "background.html",
      "about.html",
      "local_search.html",
      "local_search_preload.js",
      "permissions_popup.html",
      "livekit/**/*",
      "_locales/**",
      "image-gallery/**/*",
      "protos/*",
      "js/**",
      "ts/**/*.js",
      "ts/*.js",
      "stylesheets/*.css",
      "!js/register.js",
      "app/*",
      "preload.js",
      "about_preload.js",
      "permissions_popup_preload.js",
      "main.js",
      "audio/**/*",
      "images/**",
      "lotties/**",
      "build/assets",
      "node_modules/**",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
      "!**/node_modules/.bin",
      "!**/node_modules/*/build/**",
      "!**/*.{o,hprof,orig,pyc,pyo,rbc,c,h,m,map}",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.flowconfig,.yarn-metadata.json,.idea,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,.nyc_output,yarn.lock,.yarn-integrity}",
      "node_modules/yuv-canvas/build/shaders.js",
      "node_modules/websocket/build/Release/*.node",
      "node_modules/socks/build/*.js",
      "node_modules/socks/build/common/*.js",
      "node_modules/socks/build/client/*.js",
      "node_modules/smart-buffer/build/*.js",
      "!node_modules/@signalapp/better-sqlite3/deps/*",
      "!node_modules/@signalapp/better-sqlite3/src/*",
      "!node_modules/@signalapp/better-sqlite3/build/**/*",
      "node_modules/@signalapp/better-sqlite3/build/Release/better_sqlite3.node",
      "node_modules/human-signals/build/src/*.js",
      "lib-crypto-client/${platform}/${arch}/index.node",
      "separate-message-viewer/**/*",
      "node_modules/react-draggable/build/**/*.js"
    ]
  }
}
